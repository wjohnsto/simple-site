{{#each head.meta.images }}
{{!-- Image --}}
<meta property="og:image" content="{{url}}">
<meta property="og:image:type" content="{{default type 'image/jpeg'}}">
<meta property="og:image:width" content="{{default width 1200}}">
<meta property="og:image:height" content="{{default height 630}}">
{{!-- /Image --}}
{{/each}}